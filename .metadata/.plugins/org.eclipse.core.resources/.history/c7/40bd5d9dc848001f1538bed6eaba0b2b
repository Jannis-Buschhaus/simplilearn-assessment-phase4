package com.entity;

import java.time.LocalDateTime;

import jakarta.persistence.JoinColumn;
import jakarta.persistence.ManyToOne;

public class Visit {
	
	private int visitId;
	private LocalDateTime time;
	private String reasonForVisit;
	private int ownerId;
	private int petId;
	@ManyToOne
	@JoinColumn(name="ownerId")
	private Owner owner;
	
}
